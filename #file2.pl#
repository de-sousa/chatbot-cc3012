% ex:
% sentence_type(["hello","bot"],T).
% T unifies with "greetings".

sentence_type(L,T) :-
    type(T,L,_).

type(T) -->  usefulword(T),! ; [_],type(T),!.

usefulword("greetings") --> ["hello"]; ["hi"].
usefulword("offerhelp") --> ["help"]; ["need"].
usefulword("offerexample") --> ["example"]; ["ex"].
